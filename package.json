    "@tsconfig/recommended": "^1.0.7",
    "@types/chrome": "^0.0.269",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.0",
    "@types/puppeteer": "^5.4.7",
