    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
